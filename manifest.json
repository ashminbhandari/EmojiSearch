{
  "name": "EmojiSearch",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "<all_urls>",
    "clipboardWrite",
    "clipboardRead"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.5.1.slim.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}