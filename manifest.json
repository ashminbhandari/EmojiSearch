{
  "name": "EmojiSearch",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": [
    "activeTab",
    "<all_urls>",
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}